#compdef genzshcomp
function _genzshcomp (){
    typeset -A opt_args
    local context state line

    integer int=1

    _arguments -w -s -S \
        '(-h --help)'{-h,--help}'[show this help message and exit]' \
        ': :(dpmanip genzshcomp dataprocessor register_figure)' \
        '*::arguments:_files'
    ret=0
    return ret
}
_genzshcomp